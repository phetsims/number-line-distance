{
  "name": "number-line-distance",
  "version": "1.2.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/number-line-distance.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "phetLibs": [
      "number-line-common"
    ],
    "requirejsNamespace": "NUMBER_LINE_DISTANCE",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "NUMBER_LINE_DISTANCE/screen.explore",
      "NUMBER_LINE_DISTANCE/screen.generic"
    ],
    "simFeatures": {
      "supportedRegionsAndCultures": [
        "africa",
        "africaModest",
        "asia",
        "latinAmerica",
        "oceania",
        "random",
        "usa"
      ],
      "defaultRegionAndCulture": "random",
      "supportsDynamicLocale": true
    }
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
}